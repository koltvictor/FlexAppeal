{
  "expo": {
    "name": "FlexAppeal",
    "slug": "FA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "2",
      "UIStatusBarStyle": "UIStatusBarStyleLightContent",
      "UIViewControllerBasedStatusBarAppearance": false,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "UIApplicationSceneManifest": {
          "UIApplicationSupportsMultipleScenes": true
        }
      },
      "bundleIdentifier": "com.koltvictor.FA"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.koltvictor.FA"
    },
    "description": "FlexAppeal is an app used to build workout routines.",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "96f16341-0f56-4f01-895f-c773b3a79695"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/96f16341-0f56-4f01-895f-c773b3a79695"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useModularHeaders": true
          }
        }
      ]
    ]
  }
}
